{
  "name": "frontend",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "main": "electron/main.js",
  "author": "Chiko",
  "description": "简单易用的electron、vue3、vite模板",
  "scripts": {
    "dev": "vite",
    "h": "vite build && electron .",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run electron:serve\"",
    "electron:serve": "cross-env NODE_ENV=development electron .",
    "electron:build": "vite build && electron-builder"
  },
  "build": {
    "appId": "com.your-website.your-app",
    "productName": "ElectronApp",
    "copyright": "Copyright © 2023 <your-name>",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "pinia": "^2.0.34",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "sass": "^1.61.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
